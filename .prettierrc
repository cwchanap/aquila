{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "trimTrailingWhitespace": false,
  "insertPragma": false,
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "tabWidth": 4
      }
    }
  ],
  "plugins": []
}