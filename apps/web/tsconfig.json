{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./",
    "resolveJsonModule": true,
    "paths": {
      "@/*": ["./src/*"],
      "@aquila/assets": ["../../packages/assets"],
      "@aquila/assets/*": ["../../packages/assets/*"],
      "@aquila/dialogue/*": ["../../packages/dialogue/src/*"]
    }
  },
  "include": [
    "src",
    ".astro/types.d.ts"
  ],
  "exclude": [
    "dist"
  ]
}
