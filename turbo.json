{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "persistent": true,
      "cache": false
    },
    "dev:db": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".astro/**",
        "build/**",
        ".svelte-kit/**",
        "src-tauri/target/**",
        ".tsbuildinfo"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "outputs": []
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "outputs": []
    },
    "test:e2e": {
      "outputs": []
    },
    "test:ui": {
      "outputs": []
    },
    "test:headed": {
      "outputs": []
    },
    "test:debug": {
      "outputs": []
    },
    "test:report": {
      "outputs": []
    }
  }
}